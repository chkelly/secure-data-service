<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
    http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">

	<entity class="org.slc.sli.domain.StudentSchoolAssociation">
		<table name="student_school_association" />
		
		<attributes>
			<id name="associationId">
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="schoolId"/>
			<basic name="studentId"/>
			<basic name="entryDate"/>
			<basic name="entryGradeLevel">
				<enumerated>STRING</enumerated>
			</basic>
			<basic name="entryType">
				<enumerated>STRING</enumerated>
			</basic>
			<basic name="repeatedGrade"/>
			<basic name="classOf"/>
			<basic name="schoolChoiceTransfer"/>
			<basic name="exitWithdrawDate"/>
			<basic name="exitWithdrawType">
				<enumerated>STRING</enumerated>
			</basic>
		</attributes>
	</entity>
</entity-mappings>